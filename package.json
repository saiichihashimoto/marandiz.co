{
  "name": "marandiz.co",
  "version": "0.0.0-development",
  "homepage": "https://marandiz.co",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/marandiz.co/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/marandiz.co.git"
  },
  "bin": {
    "gsx2json": "./gsx2json.js"
  },
  "scripts": {
    "prebuild": "npm run gsx2json && cat googlesheets.json",
    "build": "cross-env REACT_APP_GOOGLE_SHEETS=\"$(cat googlesheets.json)\" react-node-scripts build --no-node",
    "postbuild": "react-snap",
    "gsx2json": "node -r dotenv/config gsx2json dotenv_config_path=.env.development.local",
    "prestart": "npm run gsx2json",
    "start": "cross-env REACT_APP_GOOGLE_SHEETS=\"$(cat googlesheets.json)\" react-node-scripts start --no-node"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "stylelint": {
    "extends": "stylelint-config-saiichihashimoto"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-ga": "2.6.0",
    "react-helmet": "5.2.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "resolve-bin": "0.4.0",
    "typeface-ibm-plex-mono": "0.0.61",
    "typeface-pt-sans": "0.0.72"
  },
  "devDependencies": {
    "create-react-node-app": "3.0.30",
    "cross-env": "5.2.0",
    "dotenv": "8.0.0",
    "eslint": "6.1.0",
    "eslint-config-saiichihashimoto": "2.0.30",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.15.1",
    "eslint-plugin-jest-formatting": "1.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-unicorn": "10.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.6.0",
    "eslint-plugin-you-dont-need-momentjs": "1.5.0",
    "husky": "3.0.3",
    "lint-my-app": "3.7.5",
    "node-sass": "4.12.0",
    "react-snap": "1.23.0",
    "stylelint-config-saiichihashimoto": "2.0.7"
  },
  "engines": {
    "node": "12.8.0"
  },
  "reactSnap": {
    "inlineCss": true,
    "viewport": {
      "width": 320,
      "height": 821
    }
  }
}
