{
  "name": "pinterestadexamples.com",
  "version": "0.0.0-development",
  "homepage": "https://pinterestadexamples.com",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/pinterestadexamples.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/pinterestadexamples.com.git"
  },
  "scripts": {
    "prebuild": "curl -s https://spreadsheets.google.com/feeds/list/$GOOGLE_SHEETS_ID/1/public/values?alt=json | node gsx2json > googlesheets.json",
    "build": "echo GOOGLE_SHEETS_ID $GOOGLE_SHEETS_ID && cat googlesheets.json && cross-env REACT_APP_GOOGLE_SHEETS=\"$(cat googlesheets.json)\" react-scripts build",
    "postbuild": "react-snap && rm googlesheets.json",
    "start": "react-scripts start"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "stylelint": {
    "extends": "stylelint-config-saiichihashimoto"
  },
  "dependencies": {
    "fuse.js": "3.4.5",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-modal": "3.8.2",
    "react-router-dom": "5.0.1",
    "react-stack-grid": "0.7.1",
    "use-debounce": "2.1.0"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "eslint-config-saiichihashimoto": "1.2.38",
    "husky": "3.0.0",
    "lint-my-app": "3.3.37",
    "node-sass": "4.12.0",
    "react-scripts": "3.0.1",
    "react-snap": "1.23.0",
    "slugify": "1.3.4",
    "stylelint-config-saiichihashimoto": "1.0.17"
  },
  "reactSnap": {
    "inlineCss": true,
    "viewport": {
      "width": 1440,
      "height": 821
    }
  }
}
