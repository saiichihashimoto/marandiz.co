{
  "name": "marandiz.co",
  "version": "0.0.0-development",
  "homepage": "https://marandiz.co",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/marandiz.co/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/marandiz.co.git"
  },
  "scripts": {
    "prebuild": "npm run gsx2json && cat googlesheets.json",
    "build": "cross-env REACT_APP_GOOGLE_SHEETS=\"$(cat googlesheets.json)\" react-node-scripts build --no-node",
    "postbuild": "react-snap",
    "gsx2json": "curl -s https://spreadsheets.google.com/feeds/list/$GOOGLE_SHEETS_ID/1/public/values?alt=json | node gsx2json > googlesheets.json",
    "prestart": "cross-env GOOGLE_SHEETS_ID=1d-OXGFqYfjU1FLXB17mYZTSD71USRMX_W0zGQWt5HCI npm run gsx2json",
    "start": "cross-env REACT_APP_GOOGLE_SHEETS=\"$(cat googlesheets.json)\" react-node-scripts start --no-node"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "stylelint": {
    "extends": "stylelint-config-saiichihashimoto"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-ga": "2.6.0",
    "react-helmet": "5.2.1",
    "react-stack-grid": "0.7.1",
    "typeface-squada-one": "0.0.72"
  },
  "devDependencies": {
    "create-react-node-app": "3.0.25",
    "cross-env": "5.2.0",
    "eslint-config-saiichihashimoto": "1.3.9",
    "husky": "3.0.1",
    "lint-my-app": "3.4.1",
    "node-sass": "4.12.0",
    "react-snap": "1.23.0",
    "stylelint-config-saiichihashimoto": "1.0.22"
  },
  "engines": {
    "node": "12.6.0"
  },
  "reactSnap": {
    "inlineCss": true,
    "skipThirdPartyRequests": true,
    "viewport": {
      "width": 1440,
      "height": 821
    }
  }
}
