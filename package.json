{
  "name": "marandiz.co",
  "version": "0.0.0-development",
  "homepage": "https://marandiz.co",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/marandiz.co/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/marandiz.co.git"
  },
  "scripts": {
    "prebuild": "npm run gsx2json",
    "build": "cat googlesheets.json && cross-env REACT_APP_GOOGLE_SHEETS=\"$(cat googlesheets.json)\" react-node-scripts build --no-node",
    "gsx2json": "curl -s https://spreadsheets.google.com/feeds/list/$GOOGLE_SHEETS_ID/1/public/values?alt=json | node gsx2json > googlesheets.json",
    "prestart": "GOOGLE_SHEETS_ID=1d-OXGFqYfjU1FLXB17mYZTSD71USRMX_W0zGQWt5HCI npm run gsx2json",
    "start": "REACT_APP_GOOGLE_SHEETS=\"$(cat googlesheets.json)\" react-node-scripts start --no-node"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "stylelint": {
    "extends": "stylelint-config-saiichihashimoto"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-stack-grid": "0.7.1"
  },
  "devDependencies": {
    "create-react-node-app": "3.0.23",
    "cross-env": "5.2.0",
    "eslint-config-saiichihashimoto": "1.3.3",
    "husky": "3.0.0",
    "lint-my-app": "3.3.44",
    "node-sass": "4.12.0",
    "stylelint-config-saiichihashimoto": "1.0.20"
  },
  "engines": {
    "node": "12.6.0"
  }
}
